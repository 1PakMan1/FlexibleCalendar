<resources>
    <declare-styleable name="CircularEventCellView">
        <attr name="event_radius" format="dimension"/>
        <attr name="event_circle_padding" format="dimension"/>
    </declare-styleable>
    <declare-styleable name="EventCountCellView">
        <attr name="event_count_radius" format="dimension"/>
        <attr name="event_background" format="color"/>
        <attr name="event_count_text_color" format="color"/>
        <attr name="event_text_size" format="dimension"/>
    </declare-styleable>
    <declare-styleable name="FlexibleCalendarView">
        <attr name="startDisplayYear" format="integer"/>
        <attr name="startDisplayMonth" format="integer"/>
        <attr name="weekDayHorizontalSpacing" format="dimension"/>
        <attr name="weekDayVerticalSpacing" format="dimension"/>
        <attr name="monthDayHorizontalSpacing" format="dimension"/>
        <attr name="monthDayVerticalSpacing" format="dimension"/>
        <attr name="weekViewBackground" format="reference"/>
        <attr name="monthViewBackground" format="reference"/>
        <attr name="showDatesOutsideMonth" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="CellState">
        <attr name="state_date_regular" format="boolean"/>
        <attr name="state_date_today" format="boolean"/>
        <attr name="state_date_selected" format="boolean"/>
        <attr name="state_date_outside_month" format="boolean"/>
    </declare-styleable>
</resources>